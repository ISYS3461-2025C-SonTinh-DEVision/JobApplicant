# Frontend Production Environment Template
# Copy this file to .env.frontend and fill in actual values
# NEVER commit the actual .env.frontend file

# API through Kong Gateway
REACT_APP_API_URL=https://your-domain.com
REACT_APP_API_GATEWAY_URL=https://your-domain.com

# Google OAuth2 (Public client ID - safe to expose)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Consul UI (for admin) - Replace with your gateway IP
REACT_APP_CONSUL_URL=http://YOUR_GATEWAY_IP:8500

# Disable debug in production
REACT_APP_DEBUG=false
